{
  "name": "flowbite-react-template-parcel-server",
  "private": true,
  "version": "0.0.0",
  "server": "dist/server.js",
  "targets": {
    "server": {
      "source": "src/server.tsx",
      "context": "react-server",
      "includeNodeModules": {
        "express": false
      }
    }
  },
  "scripts": {
    "start": "parcel",
    "build": "parcel build",
    "format": "prettier . --write",
    "format:check": "prettier . --check",
    "postinstall": "flowbite-react patch"
  },
  "dependencies": {
    "@parcel/rsc": "^2.14.4",
    "express": "^4.21.2",
    "flowbite-react": "^0.11.7",
    "react": "^19.1.0",
    "react-dom": "^19.1.0"
  },
  "devDependencies": {
    "@parcel/transformer-inline-string": "^2.14.4",
    "@tailwindcss/postcss": "^4.1.4",
    "@types/express": "^4.17.21",
    "@types/node": "^22.14.1",
    "@types/parcel-env": "^0.0.6",
    "@types/react": "^19.1.2",
    "@types/react-dom": "^19.1.2",
    "parcel": "^2.14.4",
    "prettier": "^3.5.3",
    "prettier-plugin-tailwindcss": "^0.6.11",
    "tailwindcss": "^4.1.4"
  }
}

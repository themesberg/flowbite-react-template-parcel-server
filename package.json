{
  "name": "flowbite-react-template-parcel-server",
  "private": true,
  "version": "0.0.0",
  "server": "dist/server.js",
  "targets": {
    "server": {
      "source": "src/server.tsx",
      "context": "react-server",
      "includeNodeModules": {
        "express": false
      }
    }
  },
  "scripts": {
    "start": "parcel",
    "build": "parcel build",
    "format": "prettier . --write",
    "format:check": "prettier . --check",
    "postinstall": "flowbite-react patch"
  },
  "dependencies": {
    "@parcel/rsc": "^2.14.0",
    "express": "^4.21.2",
    "flowbite-react": "^0.11.0",
    "react": "canary",
    "react-dom": "canary"
  },
  "devDependencies": {
    "@parcel/transformer-inline-string": "2.14.1",
    "@tailwindcss/postcss": "^4.0.15",
    "@types/express": "^4",
    "@types/node": "^22.10.5",
    "@types/parcel-env": "^0.0.6",
    "@types/react": "^19.0.0",
    "@types/react-dom": "^19.0.0",
    "parcel": "^2.14.0",
    "prettier": "^3.5.3",
    "prettier-plugin-tailwindcss": "^0.6.11",
    "tailwindcss": "^4.0.15"
  }
}
